---
import PortalLayout from '../../../layouts/PortalLayout.astro';
import BrandingSettings from '../../../components/admin/BrandingSettings.svelte';

const sidebarItems = [
  { label: 'Dashboard', href: '/portal/llnd' },
  { label: 'Assessment History', href: '/portal/llnd' },
  { label: 'Settings', href: '/portal/llnd/settings' },
];
---

<PortalLayout
  title="Settings - LLND Portal"
  portalName="LLND Portal"
  portalColor="blue"
  sidebarItems={sidebarItems}
>
  <div class="max-w-4xl">
    <BrandingSettings client:only="svelte" />
  </div>
</PortalLayout>
