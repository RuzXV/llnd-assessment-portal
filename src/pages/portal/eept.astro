---
import PortalLayout from '../../layouts/PortalLayout.astro';
import EeptDashboard from '../../components/eept/EeptDashboard.svelte';

const sidebarItems = [
  { label: 'Dashboard', href: '/portal/eept' },
  { label: 'Issue Assessments', href: '/portal/eept' },
  { label: 'Review Queue', href: '/portal/eept' },
  { label: 'LLND Portal', href: '/portal/llnd' },
  { label: 'Settings', href: '/portal/llnd/settings' },
